module Hand
    exposing
        ( Direction(..)
        , Hand
        , Health(..)
        , animate
        , init
        , move
        , size
        , toSvg
        )

import Html.Attributes
import Screen exposing (Position, Size)
import Svg exposing (Svg)
import Svg.Attributes
import Task


type alias Hand =
    { direction : Direction
    , health : Health
    , position : Position
    }


type Direction
    = Left
    | Right
    | Stop


type Health
    = Alive
    | Dead


init : Hand
init =
    let
        x =
            -- horizontal center
            (Screen.size.width / 2) - (width / 2)

        y =
            -- near the bottom of the screen
            Screen.size.height - (height + 6)
    in
    { direction = Stop
    , health = Alive
    , position = { x = x, y = y }
    }


size : Size
size =
    { height = height, width = width }


height : Float
height =
    51


width : Float
width =
    45


toSvg : Position -> Svg a
toSvg position =
    Svg.image
        [ Svg.Attributes.xlinkHref dataUri
        , Svg.Attributes.width (String.fromFloat width)
        , Svg.Attributes.height (String.fromFloat height)
        , Svg.Attributes.x (String.fromFloat position.x)
        , Svg.Attributes.y (String.fromFloat position.y)
        ]
        []


move : Direction -> Hand -> Hand
move direction hand =
    { hand | direction = direction }


animate : Hand -> Hand
animate hand =
    let
        amount =
            15
    in
    case hand.direction of
        Stop ->
            hand

        Left ->
            { hand
                | position =
                    increaseXPositionBy
                        (negate amount)
                        hand.position
            }

        Right ->
            { hand
                | position =
                    increaseXPositionBy
                        amount
                        hand.position
            }


increaseXPositionBy : Float -> Position -> Position
increaseXPositionBy amount position =
    let
        leftEdge =
            0

        rightEdge =
            Screen.size.width - width

        next =
            position.x + amount

        x =
            if next <= leftEdge then
                leftEdge
            else if next >= rightEdge then
                rightEdge
            else
                next
    in
    { x = x
    , y = position.y
    }


dataUri : String
dataUri =
    "data:image/png;base64,"
